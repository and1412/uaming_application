<h1>Contacts</h1>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th>Email</th>
        <th>Message</th>
        <th>See Events...</th>
      </tr>
    </thead>

    <tbody>
      <% @contacts.each do |contact| %>
        <tr>
          <td><%= contact.email %></td>
          <td><%= contact.message %></td>
          <td><%= link_to 'Show', contact %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

<%= will_paginate @contact %>
